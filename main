#include <iostream>


using namespace std;

class Lista {
    struct Wezel {
        int dane;
        Wezel* nastepny;
        Wezel(int w) : dane(w), nastepny(0) {}
    };

    Wezel* glowa;

public:
    Lista();
    void dodajNaPoczatek(int w);
    void dodajNaKoniec(int w);
    void dodajPodIndex(int index, int w);
    void usunZPoczatku();
    void usunZKonca();
    void usunPodIndexem(int index);
    void pokaz();
    void pokazOdwrotnie();
    void pokazNastepny(int index);
    void pokazPoprzedni(int index);
};
#include "dodajPoczatek.cpp"
#include "dodajKoniec.cpp"
#include "dodajIndex.cpp"
#include "usunPoczatek.cpp"
#include "usunKoniec.cpp"
#include "usunIndex.cpp"
#include "pokaz.cpp"
#include "pokazOdwrotnie.cpp"
#include "pokazNastepny.cpp"
#include "pokazPoprzedni.cpp"

Lista::Lista() : glowa(0) {}

int main() {
    Lista lista;
    lista.dodajNaPoczatek(3);
    lista.dodajNaPoczatek(2);
    lista.dodajNaPoczatek(1);
    lista.dodajNaKoniec(4);
    lista.dodajNaKoniec(5);
    lista.dodajPodIndex(2, 99);
    lista.pokaz();  // Wynik: 1 2 99 3 4 5

    lista.usunZPoczatku();
    lista.pokaz();  // Wynik: 2 99 3 4 5

    lista.usunZKonca();
    lista.pokaz();  // Wynik: 2 99 3 4

    lista.usunZPoczatku();
    lista.pokaz();  // Wynik: 99 3 4

    lista.usunPodIndexem(1); // Usuwa element o indeksie 1
    lista.pokaz();  // Wynik: 99 4

    lista.pokazOdwrotnie(); // Wynik: 4 99

    lista.pokazNastepny(0); // Wynik: 4

    lista.pokazPoprzedni(1); // Wynik: 99
    return 0;
}
